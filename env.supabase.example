# Supabase Migration Environment Configuration
# ============================================
# Copy this file to .env and update with your Supabase credentials

# Database Configuration
USE_SUPABASE=true
DATABASE_TYPE=supabase

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# File Storage Configuration
SUPABASE_STORAGE_BUCKET=master
SUPABASE_STORAGE_PRIVATE=true
MAX_FILE_SIZE=10485760

# Security
SECRET_KEY=your_super_secure_secret_key_here
DEBUG=false

# Google OAuth Configuration (if used)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Generative AI (if used)
GOOGLE_GENAI_API_KEY=AIzaSyBvnDtJz5Fw5O5x1O9AR7Myd5R5URzP33o

# Job Scraping APIs (if used)
ADZUNA_APP_ID=your_adzuna_app_id
ADZUNA_APP_KEY=your_adzuna_app_key

# Email Configuration (if used)
EMAIL_PROVIDER=resend
RESEND_API_KEY=your_resend_api_key
SMTP_FROM="Joborra <no-reply@emails.joborra.com>"

# Frontend Configuration
FRONTEND_ORIGIN=http://localhost:3000

# Production optimizations
WORKERS=3
TIMEOUT=180

# CORS Origins (for production)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173
